{
    "name": "Code Jam Tasting",
    "manifest_version": 2,
    "version": "1.0.0",
    "description": "Google Code Jam のスコアボードにソースプレビューボタンを追加",
    "permissions":["clipboardRead", "http://code.google.com/"],
    "background": {
        "scripts": [
            "BackgroundCopy.js"
        ],
        "persistent": false
    },
    "content_scripts": [{
        "matches": [
            "https://code.google.com/codejam/contest/*/scoreboard*",
            "https://www.go-hero.net/jam/*"
        ],
        "css": [
            "bootstrap.min.css",
            "prettify.css",
            "CodeJamTasting.css"
        ],
        "js": [
            "jquery.min.js",
            "prettify.js",
            "bootstrap.min.js",
            "jszip.min.js",
            "CodeJamTasting.js"
        ]
    }]
}
